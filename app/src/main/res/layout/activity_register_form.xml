<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register_and_Login.RegisterForm">

    <Button
        android:id="@+id/btnBackDftr"
        android:layout_width="42dp"
        android:layout_height="41dp"
        android:background="@drawable/icon_back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036" />

    <Button
        android:id="@+id/btn_msk"
        android:layout_width="86dp"
        android:layout_height="41dp"
        android:background="?android:attr/selectableItemBackground"
        android:fontFamily="@font/poppins_semibold"
        android:text="Masuk"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/textdaftar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Daftar"
        android:textColor="@color/Ungu1"
        android:textSize="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.215" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="Silahkan isi formulir dibawah ini"
        android:textColor="@color/Ungu1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textdaftar"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/formNama"
        android:layout_width="326dp"
        android:layout_height="53dp"
        android:background="@drawable/bg_cream"
        android:hint="Nama lengkap anda"
        android:paddingStart="20dp"
        android:textColor="@color/Brown2"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.056" />

    <EditText
        android:id="@+id/formEmail"
        android:layout_width="326dp"
        android:layout_height="53dp"
        android:background="@drawable/bg_cream"
        android:hint="Email anda"
        android:paddingStart="20dp"
        android:textColor="@color/Brown2"
        android:textSize="13dp"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formNama"
        app:layout_constraintVertical_bias="0.041" />

    <EditText
        android:id="@+id/no_Hp"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:background="@drawable/bg_cream"
        android:hint="+62"
        android:paddingStart="10dp"
        android:textColor="@color/Brown2"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.127"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formEmail"
        app:layout_constraintVertical_bias="0.045" />

    <EditText
        android:id="@+id/noHp"
        android:layout_width="252dp"
        android:layout_height="53dp"
        android:background="@drawable/bg_cream"
        android:hint="Nomor telepon"
        android:paddingStart="20dp"
        android:textColor="@color/Brown2"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toEndOf="@+id/no_Hp"
        app:layout_constraintTop_toBottomOf="@+id/formEmail"
        app:layout_constraintVertical_bias="0.045" />

    <EditText
        android:id="@+id/formsandi"
        android:layout_width="325dp"
        android:layout_height="53dp"
        android:background="@drawable/bg_cream"
        android:hint="Buat kata sandi anda"
        android:paddingStart="20dp"
        android:textColor="@color/Brown2"
        android:textSize="13dp"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/noHp"
        app:layout_constraintVertical_bias="0.062" />

    <Button
        android:id="@+id/btnDaftar"
        android:layout_width="328dp"
        android:layout_height="49dp"
        android:background="@drawable/btn_oren"
        android:fontFamily="@font/poppins_semibold"
        android:text="Daftar"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsandi"
        app:layout_constraintVertical_bias="0.738" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="Dengan mendaftar anda menyetujui"
        android:textSize="12dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/btnSyarat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formsandi"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnSyarat"
        android:layout_width="239dp"
        android:layout_height="22dp"
        android:background="?android:attr/selectableItemBackground"
        android:fontFamily="@font/poppins_semibold"
        android:text="Syarat dan ketentuan kami"
        android:textAllCaps="false"
        android:textSize="12dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/btnDaftar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>